{
  "name": "hare",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "ci": "npm run build:prod",
    "format:write": "prettier ./src/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier ./src/**/*.{ts,json} --list-different",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/flex-layout": "~10.0.0-beta.32",
    "@angular/forms": "~10.0.11",
    "@angular/material": "~10.1.3",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/platform-server": "^10.1.0",
    "@angular/router": "~10.0.11",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/entity": "~10.0.0",
    "@ngrx/router-store": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "@ngrx/store-devtools": "~10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@swimlane/ngx-dnd": "^8.2.0",
    "hammerjs": "^2.0.8",
    "lodash": "~4.17.20",
    "ngx-color-picker": "~10.0.1",
    "ngx-logger": "~4.1.9",
    "ngx-perfect-scrollbar": "~10.0.1",
    "rxjs": "~6.6.2",
    "tslib": "~2.0.0",
    "web-animations-js": "~2.3.2",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.7",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@angular/language-service": "~10.0.11",
    "@types/hammerjs": "^2.0.36",
    "@types/lodash": "~4.14.159",
    "@types/node": "~14.6.0",
    "@types/jest": "^26.0.14",
    "codelyzer": "~6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "^7.0.0",
    "rimraf": "~3.0.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7",
    "ts-node": "~9.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "webpack-bundle-analyzer": "^3.8.0",
    "karma-spec-reporter": "^0.0.32"
  }
}
